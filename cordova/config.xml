<?xml version='1.0' encoding='utf-8'?>
<widget id="nathan.fraignt.me" version="2.1.0">
    <name>IC Veenendaal</name>
    <description>
        Niet officiÃ«le app van het Ichthus College Veenendaal. Ondersteund Cijfers, Bestanden, Agenda, Rooster en slaat
        de data offline op zodat je het ook zonder internet kan inzien.
    </description>
    <author email="nathanklumpenaar11@gmail.com">
        Fraignt
    </author>

    <preference name="EnableDeveloperConsole" value="true"/>
    <preference name="UseStrictSSL" value="false"/>
    <preference name="Theme" value="@style/theme.AppTheme" />

    <preference name="android-compileSdkVersion" value="26" />
    <preference name="android-targetSdkVersion" value="26"/>

    <platform name="android">
        <icon src="drawable-mdpi/ic_launcher.png" density="mdpi" />
        <icon src="drawable-hdpi/ic_launcher.png" density="hdpi" />
        <icon src="drawable-xhdpi/ic_launcher.png" density="xhdpi" />
        <icon src="drawable-xxhdpi/ic_launcher.png" density="xxhdpi" />
        <icon src="drawable-xxxhdpi/ic_launcher.png" density="xxxhdpi" />
    </platform>

    <hook type="after_platform_add" src="copyAndroidRes.js" />


    <plugin name="tabris-plugin-firebase" spec="^1.0.0" />
    <plugin name="cordova-plugin-datetimepicker" spec="^1.0.0" />
    <plugin name="cordova-plugin-dialogs" version="^1.1.1" />
    <plugin name="cordova-plugin-fileopener" />
    <plugin name="cordova-plugin-file-opener2" />
    <plugin name="cordova-plugin-file" />
    <plugin name="cordova-plugin-file-transfer" spec="1.6"/>
    <plugin name="cordova-ourcodeworld-filebrowser" spec="https://github.com/ourcodeworld/cordova-ourcodeworld-filebrowser.git" />


</widget>
